    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yairEO/react-number-input.git"
    },
    "bugs": {
        "url": "https://github.com/yaireo/react-number-input/issues"
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            [
                "@babel/preset-react",
                {
                    "runtime": "automatic"
                }
            ]
        ]
    },
    "devDependencies": {
        "@babel/cli": "^7.14.5",
